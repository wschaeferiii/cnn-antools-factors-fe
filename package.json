{
  "name": "cnn-antools-factors-fe",
  "version": "0.1.0",
  "private": true,
  "description": "CNN Apple News Tools Factors Front End",
  "main": "server.js",
  "config": {
    "docker_build_path": ".",
    "port": "8080"
  },
  "scripts": {
    "_docker-compose-build": "npm run _generate-docker-env-file && docker-compose build",
    "_generate-docker-env-file": "echo VERSION=$(jq -r .version package.json)-local.$(uuidgen) > .env && echo PORT=8080 >> .env && echo COMPOSE_FILE=.codeship/docker-compose.yml >> .env",
    "analyze": "cnn-starter-app analyze",
    "build": "cnn-starter-app build",
    "create": "cnn-starter-app create",
    "encrypt-envvars": "jet encrypt .codeship/codeship.env .codeship/codeship.env.encrypted --key-path .codeship/codeship.aes",
    "start-docker-local": "npm run _docker-compose-build && docker-compose up -d && docker-compose ps",
    "start": "cnn-starter-app start",
    "test": "cnn-starter-app test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-antools-factors-fe"
  },
  "bugs": {
    "url": "http://tickets.turner.com/issues/?filter=29084"
  },
  "homepage": "https://github.com/cnnlabs/cnn-antools-factors-fe#readme",
  "engines": {
    "node": "8.1.2"
  },
  "dependencies": {
    "babel-register": "6.24.1",
    "babel-resolver": "1.1.0",
    "cnn-server": "git+https://github.com/cnnlabs/cnn-server.git",
    "jsonp": "0.2.1",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-helmet": "5.0.3",
    "react-hot-loader": "3.0.0-beta.6",
    "react-router-dom": "4.1.1",
    "react-toolbox": "2.0.0-beta.12"
  },
  "devDependencies": {
    "cnn-starter-app": "git+https://github.com/cnnlabs/cnn-starter-app.git",
    "jq-cli-wrapper": "0.4.0"
  },
  "license": "Apache-2.0",
  "keywords": [
    "cnn",
    "cnnlabs",
    "apple news",
    "apple news factors"
  ]
}
